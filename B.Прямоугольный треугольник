#include <iostream>
using namespace std;

int main() {
 int a, b, c;

 cin >> a >> b >> c;
 if (a <= 30000 || b <= 30000 || c <= 30000){
 if ((a + b > c) && (a + c > b) && (b + c > a)) {
 if ((a * a + b * b == c * c) || (a * a + c * c == b * b) || (b * b + c * c == a * a)) {
 cout << "YES";
 } else {
     cout << "NO";
 }
 } else {
     cout << "UNDEFINED";
 }
 }

}
