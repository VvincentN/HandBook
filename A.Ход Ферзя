#include <iostream>
using namespace std;
int main() {
 int s1, s2, a1, a2;
 cin >> s1 >> s2 >> a1 >> a2;

 if (s1 <= 0 || s1 > 8 || s2 <= 0 || s2 > 8 || a1 <= 0 || a1 > 8 || a2 <= 0 || a2 > 8) {
 cout << "wrong!";
 return 1;
 }

 if ((s1 == a1) || (s2 == a2) || (abs(s1 - a1) == abs(s2 - a2))) {
 cout << "YES";
 } else {
 cout << "NO";
 }
}
