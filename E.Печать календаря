#include <iostream>
using namespace std;

int main() {
 int n, k;
 cin >> n >> k;
 
 for (int i = 1; i < n; i++){
     cout << "   ";
 }
 
 
 for (int j = 1; j <= k; j++){
     if (j < 10){
         cout << " ";
     }
     cout << j;
     if (n == 7) {
         cout << "\n"; 
         n = 1; 
     } else {
         cout << " ";
         n++;
     }
}
}
